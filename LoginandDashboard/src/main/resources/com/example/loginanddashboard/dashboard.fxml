<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.loginanddashboard.HelloController">
    <padding>
        <Insets bottom="40.0" left="40.0" right="40.0" top="40.0" />
    </padding>
    <children>
        <Label fx:id="lblWelcome" style="-fx-font-size: 24px; -fx-text-fill: #2E86C1; -fx-font-weight: bold;" text="Ingresando..." />

        <Button mnemonicParsing="false" onAction="#onLogoutClick" text="Logout" />
    </children>
</VBox>